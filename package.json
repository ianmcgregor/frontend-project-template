{
  "name": "frontend-project-template",
  "version": "1.2.0",
  "private": false,
  "description": "A small framework-agnostic project template for front-end web apps and sites.",
  "keywords": [
    "template",
    "boilerplate",
    "js",
    "javascript",
    "gulp",
    "browserify"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "gulp",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/frontend-project-template"
  },
  "author": "ianmcgregor",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "object-fit-images": "^3.1.0",
    "postcss-object-fit-images": "^1.1.2",
    "suitcss-base": "^2.0.0",
    "usfl": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.7",
    "browserify": "^14.0.0",
    "chai": "^3.5.0",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-changed": "^1.3.2",
    "gulp-debug": "^3.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-fluent-ffmpeg": "^1.0.2",
    "gulp-if": "^2.0.2",
    "gulp-image-resize": "^0.12.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-template": "^4.0.0",
    "gulp-uglify": "^2.0.1",
    "gulp-watch": "^4.3.11",
    "gulp-webp": "^2.3.0",
    "karma": "^1.4.1",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-easings": "^0.3.0",
    "postcss-import": "^9.1.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "stylelint": "^7.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0",
    "yargs": "^6.6.0"
  },
  "vendor": {
    "lodash": "lodash"
  },
  "browser": {},
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ]
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "paths": {
    "audio": {
      "entry": "src/components/**/*.{wav,aiff,aif}",
      "dest": "dist/audio"
    },
    "connect": {
      "dir": "dist/",
      "proxy": "",
      "files": [
        "dist/**/*"
      ]
    },
    "html": {
      "entry": "src/index.html",
      "dest": "dist"
    },
    "images": {
      "entry": "src/components/**/*.{png,jpg,svg,json}",
      "entryWebp": "src/components/**/*.{png,jpg}",
      "dest": "dist/img"
    },
    "scripts": {
      "entry": "src/index.js",
      "dest": "dist/js",
      "modernizr": {
        "entry": "src/utils/modernizr/modernizr.js",
        "dest": "dist/js"
      },
      "lint": [
        "src/**/*.js",
        "task/**/*.js",
        "!src/utils/modernizr/modernizr.js"
      ]
    },
    "static": {
      "entry": [
        "src/robots.txt",
        "src/favicon-32.png",
        "src/favicon-152.png"
      ],
      "dest": "dist"
    },
    "styles": {
      "entry": "src/index.css",
      "dest": "dist/css/",
      "watch": [
        "src/**/*.css"
      ],
      "lint": [
        "src/**/*.css"
      ]
    }
  }
}
